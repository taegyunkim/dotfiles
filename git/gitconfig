[user]
	name = Taegyun Kim
	email = k.taegyun@gmail.com
[push]
	default = current
[core]
	excludesfile = ~/.gitignore
  ignorecase = false
[alias]
  tree = log --graph --decorate --pretty=oneline --abbrev-commit
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[fetch]
	prune = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[diff]
  ignoreSubmodules = dirty

